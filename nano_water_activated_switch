/* Water level sensor
*  by Hanie Kiani
*  https://electropeak.com/learn/   
*/
const int analogInPin = A0; 
int sensorValue = 0;
void setup() {
Serial.begin(9600); 
pinMode(11,OUTPUT);
}
void loop() {
sensorValue = analogRead(analogInPin); 
Serial.print("Sensor = " ); 
Serial.print(sensorValue); 
Serial.println("counts");
delay(1000); 
if (sensorValue*100/1024 > 10) digitalWrite(11, HIGH);

if (sensorValue*100/1024 < 10) digitalWrite(11, LOW);
} 
